    "ioredis": "^5.4.1",
    "jsonc": "^2.0.0",
    "jsonwebtoken": "^9.0.1",
    "logger": "^0.0.1",
    "mysql2": "^3.9.7",
    "nodemailer": "^6.9.13",
    "path": "^0.12.7",
    "pg": "^8.13.0",
    "pug": "^3.0.2",
