  "scripts": {
    "templates": "cd -r ./src/app/send-email/templates ./dist/src/app/send-email/templates",
    "prebuild": "rimraf dist",
    "build": "rimraf build && tsc -p tsconfig.build.json && npm run templates",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/main",
    "migration:create": "ts-node -P ./tsconfig.node.json node_modules/typeorm/cli.js migration:create",
    "migration:generate": "ts-node -P ./tsconfig.node.json node_modules/typeorm/cli.js migration:generate -n",
    "migration:show": "ts-node -P ./tsconfig.node.json node_modules/typeorm/cli.js migration:show",
    "migration:run": "ts-node -P ./tsconfig.node.json scripts/run-migrations.ts",
    "migration:revert": "ts-node -P ./tsconfig.node.json scripts/undo-last-migration.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:dev:db": "./src/scripts/start-db.sh"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^9.0.5",
