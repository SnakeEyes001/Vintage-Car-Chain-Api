    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^2.3.4",
    "@nestjs/core": "^9.4.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/multer": "^1.4.11",
    "bcrypt": "^5.1.1",
    "buffer": "^6.0.3",
    "cache-manager": "^5.7.3",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^5.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^7.1.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.3",
